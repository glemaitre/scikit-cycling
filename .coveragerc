# Configuration for coverage.py

[run]
branch = True
source = skcycling
include = */skcycling/*
omit =
     */skcycling/__check_build/*
     */skcycling/extraction/setup.py
     */skcycling/setup.py
     */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
